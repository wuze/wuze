
body.showmenu{
	background:url(../images/leftmenu_bg.gif) -11px top repeat-y;
}

body.hidemenu{
	background:none;
}
.pagemask{
	width:100%;
	height:100%;
	background:#000;
	position:absolute;
	z-index:100;
	filter:alpha(opacity=70);
	-moz-opacity: 0.7;
	opacity: 0.7;
	display:none;
}
.iframemask{
	position:absolute;
	z-index:80;
	top: 0;
	left: 0;
	background-color:#fff;
	border:none;
	filter:alpha(opacity=0);
	-moz-opacity:0;
	opacity: 0;
	height:100%;
	width:100%;
	display:none;
}

body.showmenu .left{
	display:block;
}
body.hidemenu .left{
	display:none;
}

body.showmenu .right{
	left:200px;
}
body.hidemenu .right{
	left:0px;
}

.left{
	position:absolute;
	top:166px;
	width:200px;
	left:0px;
	bottom:0px;
	z-index:7;
}

.left{
	*height:100%;
	*top:0;
	*bottom:0;
	*border-top:166px solid #FFF;
	*border-bottom:0px solid #FFF;
	*z-index:1;
}

.menu{
	height:100%;
}
.menu iframe{
	height:100%;
	width:99.99%;
	z-index:20;
}

.right{
	position:absolute;
	right:0;
	top:166px;
	bottom:0px;
	z-index:7;
}

.right{
	*height:100%;
	*top:0;
	*bottom:0;
	*border-top:166px solid #FFF;
	*border-bottom:0px solid #FFF;
	*z-index:1;
}
.main{
	height:100%;
}
.main iframe{
	height:100%;
	width:99.99%;
	z-index:20;
}


#header{
 position:relative;
 margin:0;
 top:0;
 left:0;
 display:block;
 width:100%;
 width:expression(document.body.clientWidth <= 1000? "1000px": "auto");
 min-width:1000px;
 height:166px;
	z-index:8;
 overflow:hidden;
 }
   
   
.top{ background:url(../images/top_bg2.jpg) repeat-x transparent; width:97%; height:138px; padding-left:3%; border-bottom:4px solid #FFFFFF;}
.top .hlogo{ float:left; padding-top:34px; width:100%;}
.top .nav{ margin-top:21px; float:left; display:block;}
.top .nav li{ float:left; margin-right:10px; width:92px; height:31px; font-weight:bold; padding-bottom:0;}
.top .nav li a.navsty{ display:block; cursor:pointer; float:left; font-size:14px; height:31px; line-height:31px; padding:0; text-decoration:none; background:url(../images/btn_bg.jpg) no-repeat scroll 0 0 transparent; color:#FFFFFF;}
.top .nav li:hover a.navsty{ background:url(../images/btn_current.jpg) no-repeat scroll 0 0 transparent; color:#0a4f69;}
.top .nav li a span{ display:block; float:left; width:92px; height:31px; padding-bottom:0; padding-top:0; text-align:center;}
.top .nav li a span.current{background:url(../images/btn_current.jpg) no-repeat scroll 0 0 transparent; color:#0a4f69;}

.topbar{ height:28px; line-height:28px; padding-left:2%; padding-right:2%; width:96%; background:#479dc0; }
.topbar .tit{ float:left; color:#FFFFFF;}
.topbar .rgbar{ float:right;}
.topbar .rgbar a{ line-height:28px;}
.topbar .rgbar .back{ background:url(../images/ico_back.jpg) no-repeat scroll 0 0 transparent; margin-right:10px; padding-left:22px;}
.topbar .rgbar .go{ background:url(../images/ico_go.jpg) no-repeat scroll 0 0 transparent; margin-right:10px; padding-left:22px;}
.topbar .rgbar .refresh{ background:url(../images/ico_refresh.jpg) no-repeat scroll 0 0 transparent; margin-right:10px; padding-left:22px;}
.topbar .rgbar .home{ background:url(../images/ico_home.jpg) no-repeat scroll 0 0 transparent; margin-right:10px; padding-left:22px;}
.topbar .rgbar .exit{ background:url(../images/ico_exit.jpg) no-repeat scroll 0 0 transparent; margin-right:10px; padding-left:22px;}





/*login begin*/
#login{ width:100%; background:#dff2f4; position:absolute; top:166px; height:100%;}
#login .theme {
	clear: both;
	padding: 10px;
	width: 70%;
	margin-top: 20px;
}

#login .theme li {
	margin-bottom: 15px;
	line-height: 20px;
	height: 20px;
}
#login .theme span {
	color: #000000;
	width: 15%;
	display: block;
	float: left;
	text-align: right;
}
input {
	color:#000;
	font-size:12px;
	font-family:Tahoma, SimSun, Arial;
}
input[type="text"],
input[type="password"] {
 	*margin:-1px 5px 0 5px;
	width:219px;
	height:20px;
	border:1px solid #a7a6aa;
	background:#FFF url("../images/input.gif") 0 0 no-repeat;
	margin-top: 0;
	margin-right: 5px;
	margin-bottom: 0;
	margin-left: 5px;
	padding: 2px;
}
* html input.text {
	width:223px;
	/*height:12px;*/
	border:1px solid #a7a6aa;
	background:#FFF url("../images/input.gif") 0 0 no-repeat;
	margin-top: -1px;
	margin-right: 5px;
	margin-bottom: 0;
	margin-left: 2px;
	padding: 3px;
	line-height: 12px;
}
input[type="radio"] {
 	*margin:-1px 0 0 1px;
}
* html input.radio {
	margin:-1px 0 0 -2px;
}
/*login end*/