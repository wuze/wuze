/*定义页面基本样式&清除默认属性 */
div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,form,fieldset,input,p,blockquote,th,td{margin:0;padding:0;}
fieldset,img{border:0;}
address,caption,cite,code,dfn,em,strong,th,var{font-style:normal;font-weight:normal;}
ol,ul {list-style:none;}
caption,th {text-align:left;}
h1,h2,h3,h4,h5,h6{margin:0; padding:0; font-size:12px; font-weight:normal;}
q:before,q:after{content:'';}
TD {vertical-align:top;}/*将td内对象置顶 */
input{border:0 none;}

a{color:#0a4f69;text-decoration:none} 
a:link {text-decoration: none;}
a:hover {color:#990000;text-decoration:underline}
.b{ font-weight:bold;}
.abl,.abl:visited,.abl a,.abl a:visited{ color:#004295; text-decoration:none;}
.abl:hover,.abl a:hover{ color:#990000;}

.mt10{ margin-top:10px;}
.mr10{ margin-right:10px;}
.clearfix:after {
       content: ".";
       visibility: hidden;
       display: block;
       height: 0;
       clear: both;
   }
     
html,body{ width:100%; height:100%;}   
body{color:#0e5d7a; font-size:12px; font-family:"微软雅黑",arial,helvetica,clean,sans-serif; overflow:hidden; z-index:1; margin:0; padding:0; }